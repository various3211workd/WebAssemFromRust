{
  "name": "create-wasm-app",
  "version": "0.1.0",
  "description": "create an app to consume rust-generated wasm packages",
  "main": "index.js",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "wp-server-up": "webpack-dev-server",
    "prosses-server-up": "node server.js",
    "wp-server-down": "webpack-dev-server",
    "prosses-server-down": "node server.js",
    "start": "start npm run wp-server-up && start npm run prosses-server-up",
    "stop": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rustwasm/create-wasm-app.git"
  },
  "keywords": [
    "webassembly",
